#include<stdio.h>
int main()
{

    int n,i,count=1;
    printf("Enter n");
    scanf("%d",&n);
    for(i=2; i<n; i++)
    {
        if(n%i==0){
            count++;
            break;
        }


    }

    if(count==1 )
        printf("prime number ");
    else
    printf("Not prime number ");

}
