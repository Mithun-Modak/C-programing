#include<stdio.h>
int main()
{

    int a[100], n, i;
    printf("enter ammount ");
    scanf("%d",&n);
    printf("enter number ");
    for(i=0; i<n; i++)
    {

        scanf("%d\n",&a[i]);
    }

    int max=a[0];
    for(i=1; i<n;i++)
    {
        if(max<a[i]);
        max=a[i];

    }
    printf("max number : %d\n",max);



    int min=a[0];
    for(i=1; i<n;i++)
    {
        if(max>a[i]);
        max=a[i];

    }
    printf("min number : %d\n",min);

}
