#include <ctype.h>
#include <stdio.h>

// Driver Code
int main()
{
     char ch;
     printf("Enter a Character : ");
     scanf("%c",&ch);

     if(ch=='a'||ch=='A'||
        ch=='e'||ch=='E'||
        ch=='i'||ch=='I'||
        ch=='o'||ch=='O'||
        ch=='u'||ch=='U')
        printf("vowel");
        else
        printf("it's consonatant");
}
